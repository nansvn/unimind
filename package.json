		"mongodb": "^6.4.0",
		"mongoose": "^8.2.1",
		"mysql2": "^3.9.2"
	},
	"devDependencies": {
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
		"concurrently": "^8.2.2",
